#!/bin/bash

MAKETHREADS=4
ARCH=arm
CROSS_COMPILE=/opt/linaro-4.9_2014.11/bin/arm-linux-gnueabihf-
INSTALL_MOD_PATH=output

make -j$MAKETHREADS ARCH=$ARCH CROSS_COMPILE=$CROSS_COMPILE INSTALL_MOD_PATH=$INSTALL_MOD_PATH msm8974_sec_defconfig VARIANT_DEFCONFIG=msm8974_sec_ks01eur_defconfig SELINUX_DEFCONFIG=selinux_defconfig
